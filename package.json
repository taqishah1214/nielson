{
  "name": "nielsen",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run htmlhint",
    "e2e": "ng e2e",
    "e2e:ci": "start-server-and-test start http://localhost:4200 cy:run",
    "stylelint": "stylelint src/**/*.scss src/**/**/*.scss --formatter verbose --config .stylelintrc",
    "htmlhint": "htmlhint src --config .htmlhintrc",
    "cy:cacheclear": "cypress cache clear",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless",
    "cy:runall": "cypress run",
    "merge": "mochawesome-merge mochawesome-report/json/*.json > mochawesome-report/json/report.json",
    "genhtml": "marge mochawesome-report/json/report.json --files mochawesome-report/html/ --inline",
    "cy:runwithreport": "npm run cy:runall; npm run merge && npm run genhtml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/localize": "~9.1.7",
    "@angular/material": "^9.2.4",
    "@angular/material-moment-adapter": "^9.2.4",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@briebug/cypress-schematic": "^3.3.0",
    "add-subtract-date": "^1.0.14",
    "ag-grid-angular": "^23.2.1",
    "ag-grid-community": "^23.2.1",
    "ag-grid-enterprise": "^23.2.1",
    "@ag-grid-enterprise/all-modules": "^23.2.1",
    "bootstrap": "^4.5.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "moment": "^2.26.0",
    "ngx-mat-select-search": "^2.2.0",
    "popper.js": "^1.16.1",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.0-rc.1",
    "rxjs": "~6.5.4",
    "stringinject": "^2.1.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.9",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@cypress/webpack-preprocessor": "5.4.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cypress": "^5.2.0",
    "htmlhint": "^0.14.1",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "rxjs-tslint": "^0.1.8",
    "start-server-and-test": "^1.11.3",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "8.0.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "xregexp": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
